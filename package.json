{
  "name": "mmo-fes-orchestration",
  "description": "A service to orchestrate actions on behalf of a export catch certicate user",
  "version": "1.10.0",
  "private": true,
  "engines": {
    "node": "~22.13.1",
    "npm": "~10.9.2"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "config.ts",
      "**/*.js",
      "**/*.d.ts",
      "dist",
      "mock_data",
      "coverage",
      "test",
      "src/validators/interfaces",
      "src/session_store/storeable.ts",
      "src/app_settings.ts",
      "src/start.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true,
    "check-coverage": true
  },
  "dependencies": {
    "@azure/event-hubs": "^5.11.4",
    "@azure/storage-blob": "^12.17.0",
    "@hapi/basic": "^7.0.2",
    "@hapi/hapi": "^21.3.12",
    "@hapi/inert": "^7.1.0",
    "@hapi/joi": "^17.1.1",
    "@hapi/joi-date": "^2.0.1",
    "@hapi/vision": "^7.0.3",
    "@hapi/wreck": "^18.1.0",
    "@hapi/yar": "^11.0.2",
    "applicationinsights": "2.9.5",
    "applicationinsights-native-metrics": "0.0.10",
    "axios": "^1.7.4",
    "bunyan": "1.8.15",
    "commander": "12.0.0",
    "cron": "3.1.7",
    "cross-env": "7.0.3",
    "cryptr": "6.3.0",
    "csvtojson": "^2.0.10",
    "dotenv": "16.4.5",
    "flat": "5.0.2",
    "form-data": "^4.0.0",
    "hapi-auth-jwt2": "^10.7.0",
    "hapi-boom-decorators": "^4.1.2",
    "ioredis": "^5.4.1",
    "joi": "^17.13.3",
    "json-query": "2.2.2",
    "jsonfile": "6.1.0",
    "jsonwebtoken": "9.0.2",
    "jwks-rsa": "3.1.0",
    "lodash": "^4.17.21",
    "mmo-ecc-pdf-svc": "2.2.1",
    "moment": "^2.30.1",
    "mongodb": "^6.6.2",
    "mongoose": "^7.5.0",
    "notifications-node-client": "^8.2.0",
    "npm-run-all": "4.1.5",
    "openid-client": "^5.6.5",
    "path-to-regexp": "6.3.0",
    "qs": "6.12.3",
    "rimraf": "^6.0.1",
    "url-parse": "^1.5.10",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@jest/types": "^29.6.3",
    "@types/bunyan": "1.8.11",
    "@types/catbox": "10.0.9",
    "@types/code": "4.0.8",
    "@types/dotenv": "8.2.0",
    "@types/es6-promise": "3.3.0",
    "@types/hapi": "^18.0.14",
    "@types/inert": "5.1.6",
    "@types/jest": "29.5.12",
    "@types/joi": "17.2.3",
    "@types/json-query": "2.2.6",
    "@types/jsonfile": "6.1.4",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "4.17.6",
    "@types/mongodb": "^4.0.7",
    "@types/nock": "11.1.0",
    "@types/node": "^20.14.10",
    "@types/qs": "^6.9.15",
    "@types/tape": "5.6.4",
    "@types/uuid": "10.0.0",
    "@types/vision": "5.3.11",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.7.3",
    "babel-jest": "29.7.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "get-stream": "^6.0.1",
    "husky": "^9.0.11",
    "jest": "^29.4.0",
    "jest-junit": "^6.3.0",
    "lab": "^18.0.2",
    "lab-transform-typescript": "3.0.1",
    "lint-staged": "^15.2.7",
    "mongodb-memory-server": "^3.1.3",
    "nock": "13.5.4",
    "nodemon": "^3.1.4",
    "nyc": "^15.1.0",
    "pre-commit": "1.2.2",
    "prettier": "^3.3.2",
    "sinon": "19.0.2",
    "source-map-support": "0.5.21",
    "tap-junit": "^5.0.3",
    "tape": "5.8.1",
    "ts-jest": "^29.2.2",
    "ts-mockito": "2.6.1",
    "ts-node": "^10.9.2",
    "typescript": "5.4.5",
    "typestyle": "2.4.0"
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "eslint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "watch:server": "cross-env NODE_ENV=development nodemon -w src/ -x ts-node src/start.ts",
    "start-debug": "node --inspect ./dist/src/start.js",
    "lint": "eslint . --ext .ts",
    "start": "npm run dev:without-auth",
    "start-with-auth": "npm run dev:with-auth",
    "dev:with-auth": "cross-env DB_CONNECTION_URI=\"mongodb://127.0.0.1:27017\" AZURE_STORAGE_PROXY_URL=http://127.0.0.1:3001 REDIS_HOST_NAME=127.0.0.1 REDIS_PORT=6379 REDIS_PASSWORD='' REDIS_TLS_ENABLED=false npm-run-all clean lint build watch:server --print-label",
    "debug:with-auth": "cross-env DB_CONNECTION_URI=\"mongodb://127.0.0.1:27017\" AZURE_STORAGE_PROXY_URL=http://127.0.0.1:3001 REDIS_HOST_NAME=127.0.0.1 REDIS_PORT=6379 REDIS_PASSWORD='' REDIS_TLS_ENABLED=false npm-run-all clean lint build start-debug --print-label",
    "dev:without-auth": "cross-env DB_CONNECTION_URI=\"mongodb://127.0.0.1:27017\" DISABLE_AUTH=true AZURE_STORAGE_PROXY_URL=http://127.0.0.1:3001 REDIS_HOST_NAME=127.0.0.1 REDIS_PORT=6379 REDIS_PASSWORD='' REDIS_TLS_ENABLED=false npm-run-all clean lint build watch:server --print-label",
    "debug:without-auth": "cross-env DB_CONNECTION_URI=\"mongodb://127.0.0.1:27017\" DISABLE_AUTH=true AZURE_STORAGE_PROXY_URL=http://127.0.0.1:3001 REDIS_HOST_NAME=127.0.0.1 REDIS_PORT=6379 REDIS_PASSWORD='' REDIS_TLS_ENABLED=false npm-run-all clean lint build start-debug --print-label",
    "test:pre-commit": "cross-env NODE_ENV=test jest",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:ci": "cross-env JEST_JUNIT_OUTPUT_DIR=test-results JEST_JUNIT_OUTPUT_NAME=junit.xml NODE_ENV=test jest --runInBand --watchAll=false --reporters=default --reporters=jest-junit --coverage --ci"
  }
}

